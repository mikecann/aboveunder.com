{
    "compilerOptions": {
      "outDir": ".",
      "module": "commonjs",
      "target": "es2017",
      "jsx": "react",
      "rootDir": "src",
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "noUnusedLocals": true,
      "typeRoots": [
        "./node_modules/@types"
    ]
    },
    "include": [
        "src"
    ],
    "exclude": [
      "node_modules",
      "out",
      "lib",
      "components",
      ".next",
      "pages",
      "tools",
      "data"
    ]
  }