    "async": "^3.1.0",
    "axios": "^0.19.0",
    "concurrently": "^5.0.0",
    "csstips": "^1.2.0",
    "csvtojson": "^2.0.10",
    "customize-cra": "^0.9.1",
    "exif": "^0.6.0",
    "fs-extra": "^8.1.0",
    "gls": "^3.0.1",
    "jimp": "^0.9.3",
    "loadash": "^1.0.0",
    "moment": "^2.29.4",
    "normalize-range": "^0.1.2",
    "query-string": "^6.9.0",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.18",
    "react-image-magnify": "^2.7.4",
    "react-markdown": "^4.2.2",
