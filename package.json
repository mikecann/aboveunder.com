{
  "name": "aboveunder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/csvtojson": "^1.1.5",
    "@types/fs-extra": "^5.0.2",
    "@types/google-map-react": "^0.23.2",
    "@types/googlemaps": "^3.30.7",
    "@types/jimp": "^0.2.1",
    "@types/lodash": "^4.14.104",
    "@types/markerclustererplus": "^2.1.33",
    "@types/react-router-dom": "^4.2.6",
    "axios": "^0.18.0",
    "concurrently": "^3.5.1",
    "csvtojson": "^1.1.9",
    "exif": "^0.6.0",
    "fs-extra": "^5.0.0",
    "jimp": "^0.2.28",
    "loadash": "^1.0.0",
    "moment": "^2.19.4",
    "normalize-range": "^0.1.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-google-maps": "^9.4.5",
    "react-image-magnify": "^2.5.2",
    "react-markdown": "^3.1.1",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.14.0",
    "semantic-ui-react": "^0.77.1"
  },
  "scripts": {
    "dev": "concurrently -n src,tools,lambda,test \"yarn dev-src\" \"yarn dev-tools\" \"yarn dev-lambda\" \"yarn test\"",
    "dev-src": "react-scripts-ts start",
    "dev-tools": "yarn build-tools -w",
    "dev-lambda": "yarn build-lambda -w",
    "test": "react-scripts-ts test --env=jsdom -u",
    "build": "yarn build-src && yarn build-tools && yarn build-lambda",
    "build-src": "react-scripts-ts build",
    "build-tools": "tsc -p ./tools/tsconfig.json",
    "build-lambda": "tsc -p ./src/lambda/tsconfig.json",
    "deploy": "yarn build-tools && yarn build-lambda && yarn generateProductsJson && yarn build-src && yarn test",
    "generateProductsJson": "node dist/tools/generateProductsJson",
    "importFitz": "node dist/tools/importFitzPrice",
    "importPrintful": "node dist/tools/importPrintfulPrice",
    "detectAndAddPrints": "node dist/tools/detectAndAddPrints",
    "generateThumbs": "node dist/tools/generateThumbs",
    "processImages": "node dist/tools/processImages"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.2",
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.4",
    "ts-node": "^4.0.1",
    "typescript": "^2.8.1"
  }
}
